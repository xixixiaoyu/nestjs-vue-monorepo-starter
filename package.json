{
  "name": "nest-vue-template",
  "private": true,
  "version": "0.2.0",
  "packageManager": "pnpm@9.12.0",
  "devDependencies": {
    "typescript": "^5.9.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "husky": "^9.1.5",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "lint-staged": "^15.2.9",
    "turbo": "latest",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint-plugin-vue": "^9.22.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.2.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@web",
    "dev:server": "turbo run dev --filter=@server",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "pnpm lint:root && pnpm lint:packages",
    "lint:fix": "pnpm lint:root -- --fix && pnpm lint:packages -- --fix",
    "lint:root": "eslint . --ext .ts,.tsx,.js,.vue -c packages/eslint-config-custom/index.js",
    "lint:packages": "turbo run lint",
    "check": "pnpm lint && pnpm typecheck",
    "check:prepush": "pnpm lint:fix && pnpm lint",
    "test": "echo \"no tests yet\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
